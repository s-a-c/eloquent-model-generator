{
    "name": "s-a-c/eloquent-model-generator",
    "description": "A Laravel package for generating Eloquent models from SQLite database schema",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "s-a-c",
            "email": "s-a-c@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/console": "^12.0",
        "illuminate/database": "^12.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.5",
        "brianium/paratest": "^7.7",
        "driftingly/rector-laravel": "^2.0",
        "fakerphp/faker": "^1.24",
        "friendsofphp/php-cs-fixer": "^3.70",
        "infection/infection": "^0.29",
        "larastan/larastan": "^3.1",
        "laravel/pail": "^1.2",
        "laravel/pint": "^1.21",
        "laravel/sail": "^1.41",
        "laravel/tinker": "^2.10",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "orchestra/testbench": "^10.0",
        "peckphp/peck": "^0.1.2",
        "pestphp/pest": "^3.7",
        "pestphp/pest-plugin": "^3.0",
        "pestphp/pest-plugin-faker": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.1",
        "pestphp/pest-plugin-livewire": "^3.0",
        "pestphp/pest-plugin-mutate": "^3.0",
        "pestphp/pest-plugin-stressless": "^3.1",
        "pestphp/pest-plugin-type-coverage": "^3.3",
        "phpmd/phpmd": "^2.15",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpdoc-parser": "^2.1",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-mockery": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/php-code-coverage": "^11.0",
        "phpunit/phpunit": "^11.5.3",
        "psalm/attributes": "^0.1.0",
        "psalm/plugin-laravel": "^3.0",
        "psalm/plugin-mockery": "^1.2",
        "psalm/plugin-phpunit": "^0.19.2",
        "rector/extension-installer": "^0.11.2",
        "rector/rector": "^2.0",
        "rector/type-perfect": "^2.0",
        "soloterm/solo": "^0.4.0",
        "spatie/laravel-blade-comments": "^1.3",
        "spatie/laravel-horizon-watcher": "^1.1",
        "spatie/laravel-ray": "^1.39",
        "spatie/laravel-web-tinker": "^1.10",
        "vimeo/psalm": "^6.8"
    },
    "autoload": {
        "psr-4": {
            "SAC\\EloquentModelGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SAC\\EloquentModelGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SAC\\EloquentModelGenerator\\ModelGeneratorServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage",
        "test:mutation": "infection",
        "cs:check": "php-cs-fixer fix --dry-run --diff",
        "cs:fix": "php-cs-fixer fix",
        "stan": "phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "rector/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
