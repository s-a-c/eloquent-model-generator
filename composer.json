{
    "name": "sac/eloquent-model-generator",
    "description": "An advanced Eloquent model generator for Laravel with comprehensive static analysis and code quality tools",
    "type": "library",
    "keywords": ["laravel", "eloquent", "model", "generator", "migration"],
    "license": "MIT",
    "authors": [
        {
            "name": "SAC",
            "email": "embrace.s0ul@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/console": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/support": "^11.0",
        "laravel/framework": "^11.0",
        "laravel/installer": "^5.0",
        "laravel/prompts": "^0.3"
    },
    "require-dev": {
        "alebatistella/duskapiconf": "^1.0",
        "barryvdh/laravel-debugbar": "^3.0",
        "barryvdh/laravel-ide-helper": "^3.0",
        "beyondcode/laravel-query-detector": "^2.0",
        "beyondcode/tinkerwell-helper": "^1.0",
        "dealerdirect/phpcodesniffer-composer-installer": "*",
        "driftingly/rector-laravel": "^2.0",
        "fakerphp/faker": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "imanghafoori/laravel-anypass": "^1.0",
        "kevincobain2000/laravel-erd": "^1.0",
        "larastan/larastan": "^3.0",
        "laravel/dusk": "^8.0",
        "laravel/pail": "^1.2",
        "laravel/pint": "^1.0",
        "laravel/prompts": "^0.3",
        "laravel/sail": "^1.0",
        "mockery/mockery": "^1.6",
        "mongodb/laravel-mongodb": "^5.1",
        "mongodb/mongodb": "^1.20",
        "nikic/php-parser": "^5.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.9",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-faker": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "pestphp/pest-plugin-livewire": "^3.0",
        "pestphp/pest-plugin-mutate": "^3.0",
        "pestphp/pest-plugin-stressless": "^3.0",
        "pestphp/pest-plugin-type-coverage": "^3.0",
        "pestphp/pest": "^3.0",
        "phpmd/phpmd": "^2.0",
        "phpmetrics/phpmetrics": "^3.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpdoc-parser": "^2.0",
        "phpstan/phpstan-mockery": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.0",
        "rector/rector": "^2.0",
        "rector/swiss-knife": "^2.1",
        "soloterm/solo": "^0.3",
        "spatie/laravel-error-solutions": "^1.0",
        "spatie/laravel-horizon-watcher": "^1.0",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-ray": "^1.0",
        "tomasvotruba/class-leak": "^2.0",
        "tomasvotruba/type-coverage": "^2.0",
        "vimeo/psalm": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "SAC\\EloquentModelGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SAC\\EloquentModelGenerator\\Tests\\": "tests/",
            "App\\": "tests/tmp/app/App/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "pest --coverage --min=80",
        "test:types": "pest --type-coverage --min=90",
        "test:parallel": "pest --parallel",
        "test:edge": "pest --group=edge-cases",
        "analyze": "@php artisan analyze",
        "fix": "@php artisan fix",
        "phpstan": "phpstan analyse --level=max",
        "psalm": "psalm --show-info=true --threads=4",
        "phpmd": "phpmd src ansi phpmd.xml",
        "style": [
            "@cs-check"
        ],
        "cs-check": "php-cs-fixer fix --dry-run --diff",
        "cs-fix": "php-cs-fixer fix",
        "metrics": "phpmetrics --report-html=build/metrics src",
        "rector-dry": "rector process --dry-run",
        "rector": "rector process",
        "type-coverage": "vendor/bin/type-coverage analyse src --level=3",
        "class-leak": "vendor/bin/class-leak analyse src",
        "check-all": [
            "@test:parallel",
            "@test:types",
            "@test:edge",
            "@analyze",
            "@style",
            "@metrics",
            "@type-coverage"
        ],
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "rector-laravel": "vendor/bin/rector --config=rector-laravel.php"
    },
    "scripts-descriptions": {
        "test": "Run Pest tests",
        "test:coverage": "Run tests with code coverage (minimum 80%)",
        "test:types": "Run tests with type coverage analysis (minimum 90%)",
        "test:parallel": "Run tests in parallel",
        "test:edge": "Run edge case tests specifically",
        "analyze": "Run all static analysis tools with interactive prompts",
        "fix": "Run automated fixes for static analysis issues with interactive prompts",
        "phpstan": "Run PHPStan static analysis at maximum level",
        "psalm": "Run Psalm static analysis with multi-threading",
        "phpmd": "Run PHP Mess Detector",
        "style": "Run all code style checks",
        "cs-check": "Check code style with PHP-CS-Fixer",
        "cs-fix": "Fix code style with PHP-CS-Fixer",
        "metrics": "Generate code metrics report",
        "rector-dry": "Run Rector in dry-run mode",
        "rector": "Run Rector to apply changes",
        "type-coverage": "Run type coverage analysis with strict level",
        "class-leak": "Run class leak analysis",
        "check-all": "Run all checks and tests including edge cases"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SAC\\EloquentModelGenerator\\Providers\\EloquentModelGeneratorServiceProvider"
            ]
        },
        "phpstan": {
            "includes": [
                "vendor/larastan/larastan/extension.neon"
            ],
            "parameters": {
                "level": 8,
                "tmpDir": "build/phpstan",
                "rootDir": ".",
                "customRulesetUsed": true,
                "bootstrapFiles": [],
                "autoload_files": [],
                "autoload_directories": [],
                "scanFiles": [],
                "scanDirectories": [],
                "checkModelProperties": true,
                "checkPhpDocMissingReturn": true,
                "checkUnionTypes": true,
                "reportUnmatchedIgnoredErrors": false,
                "treatPhpDocTypesAsCertain": false,
                "parallel": {
                    "maximumNumberOfProcesses": 1
                },
                "excludePaths": [
                    "tests/tmp/*",
                    "tests/*",
                    "build/*",
                    "vendor/*",
                    "*.blade.php"
                ]
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true,
            "pestphp/pest-plugin-mutate": true,
            "phpstan/extension-installer": true,
            "tomasvotruba/class-leak": true,
            "tomasvotruba/type-coverage": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
