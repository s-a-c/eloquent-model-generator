{
    "source": {
        "directories": [
            "src"
        ],
        "excludes": [
            "vendor",
            "tests"
        ]
    },
    "logs": {
        "text": "infection.log",
        "html": "infection.html",
        "summary": "summary.log",
        "json": "infection.json",
        "perMutator": "per-mutator.md"
    },
    "tmpDir": "build/infection",
    "mutators": {
        "@default": true,
        "@function_signature": true,
        "@number": true,
        "@operator": true,
        "@regex": true,
        "@removal": true,
        "@return_value": true,
        "@sort": true,
        "@unwrap": true,
        "@cast": true,
        "@identical": true
    },
    "testFramework": "pest",
    "bootstrap": "./vendor/autoload.php",
    "initialTestsPhpOptions": "-d memory_limit=4G",
    "minMsi": 85,
    "minCoveredMsi": 90,
    "parallel": {
        "processes": 4,
        "coverage": 4
    },
    "testFrameworkOptions": "--configuration=phpunit.xml --stop-on-failure",
    "ignoreMsiWithNoMutations": true,
    "git": {
        "binary": "git",
        "repositoryUrl": "https://github.com/sac/eloquent-model-generator.git"
    }
}
