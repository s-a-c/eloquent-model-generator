# Model Generator Configuration
EMG_NAMESPACE=App\\Models
EMG_OUTPUT_PATH=app/Models
EMG_BASE_CLASS=Illuminate\\Database\\Eloquent\\Model

# Model Generation Features
EMG_WITH_RELATIONSHIPS=true
EMG_WITH_VALIDATION=true
EMG_WITH_SOFT_DELETES=false

# Table Filtering
EMG_EXCLUDE_TABLES=migrations,failed_jobs,password_resets,personal_access_tokens
EMG_ONLY_TABLES=

# Performance Settings
EMG_USE_CACHE=true
EMG_CACHE_TTL=3600
EMG_CONCURRENCY=4

# Database Analysis Settings
EMG_ANALYZE_FOREIGN_KEYS=true
EMG_ANALYZE_INDEXES=true
EMG_ANALYZE_COLUMN_TYPES=true

# Code Style Settings
EMG_PSR12_COMPLIANCE=true
EMG_WITH_DOCBLOCKS=true
EMG_WITH_TYPE_HINTS=true

# Relationship Settings
EMG_DETECT_POLYMORPHIC=true
EMG_DETECT_MANY_TO_MANY=true
EMG_DETECT_HAS_THROUGH=true

# Validation Settings
EMG_VALIDATION_FROM_DB_RULES=true
EMG_VALIDATION_INCLUDE_FORMREQUEST=true

# Cache Settings
EMG_CACHE_DRIVER=file
EMG_CACHE_PREFIX=model_generator_
